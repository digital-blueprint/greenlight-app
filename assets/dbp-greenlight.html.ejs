                    outline:none !important;
                    outline-width: 0 !important;
                    box-shadow: none;
                    -moz-box-shadow: none;
                    -webkit-box-shadow: none;
                    box-shadow: 0px 0px 4px 2px var(--dbp-accent-dark);
