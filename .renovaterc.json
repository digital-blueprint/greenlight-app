{
    "extends": ["config:base", "group:allNonMajor", "schedule:weekly"],
    "ignorePresets": [":prHourlyLimit2"],
    "rangeStrategy": "update-lockfile",
    "composer": {
        "enabled": false
    },
    "cloneSubmodules": true,
    "ignorePaths": ["**/bench/**"],
    "packageRules": [
        {
          "matchPackageNames": ["pdfjs-dist"],
          "allowedVersions": "<3"
        }
    ],
    "enabled": false
}
